%encoding "UTF-8"
%version 1.0


# prefixes

%prefix iso  <http://psi.topicmaps.org/iso13250/>
%prefix psi  <http://psi.topicmapslab.de/>
%prefix xsd  <http://www.w3.org/2001/XMLSchema#>
%prefix tmdm  <http://psi.topicmaps.org/iso13250/model/>
%prefix tmcl  <http://psi.topicmaps.org/tmcl/>


# includes

%include  <http://www.isotopicmaps.org/tmcl/templates.ctm>



# template definitions
# topic definitions
psi:age  
	matches-regexp(".*");
	has-datatype("http://www.w3.org/2001/XMLSchema#string");
	 isa tmcl:occurrence-type;
	 -  tmdm:topic-name : "Age".

psi:adult  
	 isa tmcl:topic-type;
	 ako psi:person;
	 -  tmdm:topic-name : "Adult".

psi:child  
	has-occurrence(psi:age,"1","1");
	 isa tmcl:topic-type;
	 ako psi:person;
	 -  tmdm:topic-name : "Child".

psi:person  
	has-name(tmdm:topic-name,"1","1");
	 isa tmcl:topic-type;
	 -  tmdm:topic-name : "Person".

